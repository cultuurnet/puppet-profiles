#!/bin/bash

table_name=ownership_search
date_yesterday=$(date -d "yesterday" +%Y-%m-%d)

/usr/local/bin/sling run --src-conn '<%= @database_name %>' --src-stream "<%= @database_name %>.${table_name}" --tgt-conn 'publiq-ingestion' --tgt-object "gs://publiq-ingestion/udb/${table_name}/v1/${date_yesterday}/${table_name}.parquet"

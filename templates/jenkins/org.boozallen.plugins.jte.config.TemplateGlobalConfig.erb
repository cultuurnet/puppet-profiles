<?xml version='1.1' encoding='UTF-8'?>
<org.boozallen.plugins.jte.config.TemplateGlobalConfig plugin="templating-engine@1.5.2">
  <tier>
    <scm class="hudson.plugins.git.GitSCM" plugin="git@4.2.2">
      <configVersion>2</configVersion>
      <userRemoteConfigs>
        <hudson.plugins.git.UserRemoteConfig>
          <url>https://bitbucket.org/cultuurnet/jenkins-pipeline-libraries.git</url>
          <credentialsId>${bitbucketcredential}</credentialsId>
        </hudson.plugins.git.UserRemoteConfig>
      </userRemoteConfigs>
      <branches>
        <hudson.plugins.git.BranchSpec>
          <name>*/master</name>
        </hudson.plugins.git.BranchSpec>
      </branches>
      <doGenerateSubmoduleConfigurations>false</doGenerateSubmoduleConfigurations>
      <submoduleCfg class="list"/>
      <extensions/>
    </scm>
  </tier>
</org.boozallen.plugins.jte.config.TemplateGlobalConfig>
<%- if ! @configuration.empty? -%>
---
credentials:
  system:
    domainCredentials:
    - credentials:
      <%- [@configuration].flatten.each do |credential| -%>
      - aws:
          id: '<%= credential['id'] %>'
          scope: GLOBAL
          accessKey: '<%= credential['access_key'] %>'
          secretKey: '<%= credential['secret_key'] %>'
      <%- end -%>
<%- end -%>

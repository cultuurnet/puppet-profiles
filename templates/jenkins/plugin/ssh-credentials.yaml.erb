credentials:
  system:
    domainCredentials:
    - credentials:
      <%- @configuration['credentials'].each do |credential| -%>
      - basicSSHUserPrivateKey:
          id: "<%= credential['id'] %>"
          privateKeySource:
            directEntry:
              privateKey: "<%= credential['private_key'] %>"
          scope: GLOBAL
          username: "<%= credential['id'] %>"
      <%- end -%>
